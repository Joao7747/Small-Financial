<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="678.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="telassmallfinancial.MenuCursosController">
    <stylesheets>
        <URL value="@ComboBoxStyle.css" />
    </stylesheets>
   <children>
      <TableView fx:id="tvCurso" editable="true" layoutX="14.0" layoutY="143.0" prefHeight="185.0" prefWidth="646.0" stylesheets="@TableStyle.css">
         <columns>
            <TableColumn fx:id="tcIdCurso" prefWidth="33.0" text="Id" />
            <TableColumn fx:id="tcNome" prefWidth="139.0" text="Nome" />
            <TableColumn fx:id="tcDescricao" prefWidth="126.0" text="Descrição" />
            <TableColumn fx:id="tcDataLimite" prefWidth="191.0" text="Data de Encerramento" />
            <TableColumn fx:id="tcLink" prefWidth="155.0" text="Link" />
         </columns>
      </TableView>
      <Button fx:id="btnInserir" layoutX="70.0" layoutY="40.0" mnemonicParsing="false" onAction="#TelaInserirCurso" prefHeight="38.0" prefWidth="121.0" styleClass="buttonBlack" stylesheets="@buttonStyle.css" text="Inserir" textFill="WHITE">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <Button fx:id="btnVoltar" layoutX="487.0" layoutY="40.0" mnemonicParsing="false" onAction="#Voltar" prefHeight="38.0" prefWidth="121.0" styleClass="buttonWhite" stylesheets="@buttonStyle.css" text="Voltar">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <Label fx:id="lblContas" layoutX="302.0" layoutY="47.0" minHeight="16" minWidth="69" prefHeight="48.0" prefWidth="83.0" text="Cursos">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="btnEditar" layoutX="14.0" layoutY="337.0" mnemonicParsing="false" onAction="#EditarCurso" prefHeight="27.0" prefWidth="319.0" styleClass="buttonBlack" stylesheets="@buttonStyle.css" text="Editar" textFill="WHITE" />
      <Button fx:id="btnExcluir" layoutX="337.0" layoutY="337.0" mnemonicParsing="false" onAction="#deleta" prefHeight="27.0" prefWidth="328.0" styleClass="buttonBlack" stylesheets="@buttonStyle.css" text="Excluir" textFill="WHITE" />
   </children>
</AnchorPane>
